{
  "bracketSameLine": true,
  
  "semi": false,
  "useTabs": true,
  "trailingComma": "all",
  "singleAttributePerLine": true,
  "requirePragma": true,
  "quoteProps": "as-needed",
  "bracketSpacing": true,
  "arrowParens": "always",
  "singleQuote": true,
  "spaceAroundOperators": true,
  "printWidth": 100,
  "proseWrap": "preserve",
  "endOfLine": "auto",
  "tabWidth": 2,
  "insertPragma":true,
  "experimentalTernaries": true
}